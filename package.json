{
  "name": "shop.co",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache .",
    "lint:fix": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache . --fix",
    "lint:scss": "stylelint --cache --cache-location ./node_modules/.cache/.stylelintcache src/**/*.scss",
    "lint:scss:fix": "stylelint --cache --cache-location ./node_modules/.cache/.stylelintcache --fix src/**/*.scss",
    "preview": "vite preview",
    "prettify": "prettier --loglevel warn --cach --write '{src/*/*,*}.{ts,tsx,js,json,yml,scss}' '!./node_modules/**/*' '!./dist/**/*'",
    "prepare": "husky init | chmod ug+x .husky/*"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "sass-embedded": "^1.83.0",
    "stylelint": "^16.12.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^5.4.10"
  }
}
